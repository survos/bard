{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">= 8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo_sqlite": "*",
        "ext-sqlite3": "*",
        "ext-zip": "*",
        "adminkit/adminkit": "dev-master",
        "api-platform/core": "^2.5",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "easyrdf/easyrdf": "*",
        "friendsofsymfony/elastica-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "^3.1.1",
        "gajus/dindent": "^2.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "jolicode/elastically": "^1.2",
        "knplabs/knp-menu-bundle": "^3.0",
        "monolog/monolog": "^2",
        "nelmio/cors-bundle": "^2.1",
        "nyholm/psr7": "^1.3",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "6.*",
        "sentry/sentry-symfony": "dev-master",
        "survos/base-bundle": "^2",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/flex": "^2",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/intl": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^6.0",
        "symfony/process": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/string": "^6.0",
        "symfony/translation-contracts": "*",
        "symfony/twig-bundle": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/ux-twig-component": "^2",
        "symfony/validator": "^6.0",
        "symfony/web-link": "^6.0",
        "symfony/web-profiler-bundle": "^6.0",
        "symfony/webpack-encore-bundle": "^v1.14",
        "symfony/workflow": "^6.0",
        "symfony/yaml": "^6.0",
        "symfonycasts/verify-email-bundle": "^1.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.4",
        "twig/twig": "^2.12|^3.0",
        "umbrella2/adminbundle": "^6.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.99",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "compile": [
            "# wget https://museo.survos.com.s3.amazonaws.com/loc.db.zip --no-check-certificate && unzip loc.db.zip",
            "@php  -i | grep memory_limit",
            "# bin/create-admins.sh"
        ],

        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "survos_base_bundle": {
            "type": "vcs",
            "url": "git@github.com:survos/BaseBundle.git"
        },
        "adminkit":         {
            "type": "package",
            "package": {
                "name": "adminkit/adminkit",
                "version": "master",
                "dist": {
                    "url": "https://github.com/adminkit/adminkit/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    }
}
