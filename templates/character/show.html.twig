{% extends 'character/base.html.twig' %}

{% block page_title %}Character {{ character }}{% endblock %}

{% block body %}

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ character.id }}</td>
            </tr>
            <tr>
                <th>Scenes</th>
                <td>{{ character. }}</td>
            </tr>

            <tr>
                <th>Name</th>
                <td>{{ character.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ character.description }}</td>
            </tr>
        </tbody>
    </table>


{% endblock %}
